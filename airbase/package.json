{
  "name": "airbase-gscript",
  "private": true,
  "scripts": {
    "build": "ts-node --transpile-only ./scripts/esbuild.ts && yarn de-iffe",
    "dist": "yarn build && cp appsscript.json dist/",
    "de-iffe": "cat dist/index.js | head -n -1 > dist/tmp.js && echo '});' >> dist/tmp.js && mv dist/tmp.js dist/index.js"
  },
  "dependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.55",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "esbuild": "^0.17.11",
    "esbuild-gas-plugin": "^0.5.0",
    "gas-webpack-plugin": "^2.3.0",
    "shelljs": "^0.8.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsc-loader": "^1.0.4",
    "typescript": "^4.8.2",
    "typescript-language-server": "^1.1.2"
  }
}
